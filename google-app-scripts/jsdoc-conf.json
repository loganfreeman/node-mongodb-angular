{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.gs(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
                "plugins/summarize",
                "plugins/markdown"
                ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",               // same as -e utf8
        "destination": "./docs/",          // same as -d ./out/
        "recurse": true,
        "readme": "./README.md"
    }
}